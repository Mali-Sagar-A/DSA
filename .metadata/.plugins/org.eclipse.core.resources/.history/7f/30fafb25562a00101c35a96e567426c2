package Recursion;

public class Pattern {
	
	public static void Pattern1(int n)
	{
		if(n<1)
		{
			return;
		}
		Pattern1(n-1);
		System.out.println(" ");
		for(int i=1;i<=n;i++)
		{
			System.out.println("*");
		}
		return;
		
	}
	public static void main(String[] args) {
		Pattern p=new Pattern();
		p.Pattern1(5);
	}
}
