package Hashing;

import stack.Student;

public class Record_Info {
	
	private enum Record_type{EMPTY,OCCUPIED,DELETED}
	private  Record_type states;
	private Student ref;
	public Record_type getStates() {
		return states;
	}
	public void setStates(Record_type states) {
		this.states = states;
	}
	public Student getRef() {
		return ref;
	}
	public void setRef(Student ref) {
		this.ref = ref;
	}
	
	
	
	
	
	
	
	
	

}
